@model Microsoft.AspNetCore.Identity.IdentityUser
@{
    ViewData["Title"] = "EditUser";
}

<h1>EditUser</h1>

<form asp-controller="ManagerUser" asp-action="EditUser" method="post">
    <div asp-validation-summary="ModelOnly"></div>

    <div class="from-group">
        <lable class="control-label" asp-for="UserName"></lable>
        <input class="from-control" asp-for="UserName" />
    </div>

    <input type="hidden" asp-for="Id"/>

    <button type="submit" class="btn btn-Primary">Submit</button>
</form>

